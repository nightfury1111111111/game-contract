{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VoteResponse",
  "type": "object",
  "properties": {
    "vote": {
      "anyOf": [
        {
          "$ref": "#/definitions/VoteInfo"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "definitions": {
    "Vote": {
      "type": "string",
      "enum": [
        "yes",
        "no",
        "abstain",
        "veto"
      ]
    },
    "VoteInfo": {
      "description": "Returns the vote (opinion as well as weight counted) as well as the address of the voter who submitted it",
      "type": "object",
      "required": [
        "vote",
        "voter",
        "weight"
      ],
      "properties": {
        "vote": {
          "$ref": "#/definitions/Vote"
        },
        "voter": {
          "type": "string"
        },
        "weight": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    }
  }
}
